            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/iv_profile_3"
            app:layout_constraintTop_toTopOf="@+id/iv_profile_3" />

        <TextView
            android:id="@+id/tv_role_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="@string/ml"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/iv_profile_3"
            app:layout_constraintTop_toBottomOf="@+id/tv_nama_3" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/iv_profile_4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/pp_reyhan"
            app:civ_border_color="@color/green_500"
            app:civ_border_width="2dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile_3" />

        <TextView
            android:id="@+id/tv_nama_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/reyhan"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/iv_profile_4"
            app:layout_constraintTop_toTopOf="@+id/iv_profile_4" />

        <TextView
            android:id="@+id/tv_role_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="@string/ml"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/iv_profile_5"
            app:layout_constraintTop_toBottomOf="@+id/tv_nama_4" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/iv_profile_5"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/pp_riyandi"
            app:civ_border_color="@color/green_500"
            app:civ_border_width="2dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile_4" />

        <TextView
            android:id="@+id/tv_nama_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
